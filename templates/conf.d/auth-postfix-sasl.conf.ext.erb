# Authentication for Postfix SASL. Included from auth.conf.
#
# http://www.postfix.org/SASL_README.html#server_dovecot

auth default {
  mechanisms = plain login
    passdb pam {
    }
    userdb passwd {
    }
    socket listen {
    client {
      path = /var/spool/postfix/private/auth
      mode = 0660
      user = postfix
      group = postfix
    }
  }
}
